{
    "name": "alirezamires/dummy-server",
    "type": "library",
    "version": "dev-master",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": [
        "dev",
        "dummy",
        "mock",
        "server"
    ],
    "homepage": "https://github.com/alirezamires/php-dummy-server",
    "license": "MIT",
    "description": "The PHP Dummy Server is a library that allows you to create a mock server environment for testing applications that rely on external APIs or services",
    "autoload": {
        "files": [
            "root_dir.php"
        ],
        "psr-4": {
            "Alirezamires\\DummyServer\\": "src/"
        }
    },
    "scripts": {
        "run-server": "@php -S localhost:8080 index.php",
        "test" : "vendor/bin/phpunit --testdox tests"
    },
    "authors": [
        {
            "name": "Alireza Mires",
            "email": "alirezamires@gmail.com",
            "homepage": "https://github.com/alirezamires"
        }
    ],
    "config":{
        "process-timeout":0
    },
    "require": {
        "ext-zip": "*",
        "spatie/once": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "11"
    }
}
