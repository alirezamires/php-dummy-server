{
    "name": "alirezamires/dummy-server",
    "type": "library",
    "version": "dev-master",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": [
        "dev",
        "dummy",
        "mock",
        "server"
    ],
    "homepage": "https://github.com/alirezamires/php-dummy-server",
    "license": "MIT",
    "description": "Dummy Server allows you to receive all request via HTTP or HTTPS, such as a REST or RPC service.",
    "autoload": {
        "files": [
            "root_dir.php"
        ],
        "psr-4": {
            "Alirezamires\\DummyServer\\": "src/"
        }
    },
    "scripts": {
        "run-server": "@php -S localhost:8080 index.php"
    },
    "authors": [
        {
            "name": "Alireza Mires",
            "email": "alirezamires@gmail.com",
            "homepage": "https://github.com/alirezamires"
        }
    ],
    "config":{
        "process-timeout":0
    },
    "require": {
        "ext-zip": "*"
    }
}
